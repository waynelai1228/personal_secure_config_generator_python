{
  "Banner": {
    "type": "String",
    "default_value": "none"
  },
  "Ciphers": {
    "type": "Multiple-Options",
    "options": ["3des-cbc", "aes128-cbc", "aes192-cbc", "aes256-cbc",
      "aes128-ctr", "aes192-ctr", "aes256-ctr", "arcfour128",
      "arcfour256", "arcfour", "flowfish-cbc", "cast128-cbc"]
  },
  "ClientAliveCountMax": {
    "type": "Number",
    "default_value": "3"
  },
  "ClientAliveInterval": {
    "type": "Number",
    "default_value": "0"
  },
  "GSSAPIAuthentication": {
    "type": "Single-Option", 
    "default_value": "no",
    "options": ["yes", "no"]
  },
  "GSSAPIKeyExchange": {
    "type": "Single-Option", 
    "default_value": "no",
    "options": ["yes", "no"]
  },
  "GSSAPICleanupCredentials": {
    "type": "Single-Option", 
    "default_value": "yes",
    "options": ["yes", "no"]
  },
  "GSSAPIStrictAcceptorCheck": {
    "type": "Single-Option",
    "default_value": "no",
    "options": ["yes", "no"]
  },
  "HostbasedAuthentication": {
    "type": "Single-Option",
    "default_value": "no",
    "options": ["yes", "no"]
  },
  "HostKey": {
    "type": "String",
    "default_value": "/etc/ssh/ssh_host_key"
  }
}
